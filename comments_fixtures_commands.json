from professors.models import Professor
from users.models import User
from comments.models import Comment

import json
import datetime
u = open('users.json')
c = open('webappli_notaso.json')
users = json.load(u)
comments = json.load(c)
for i in comments:
     user_id = i['user_id']
     for j in users:
          if user_id == j['fields']['notaso_user_id']:
             email = j['fields']['email']
             jd = i['johnDoe']
             if jd == 1:
                 jd = True
             else:
                 jd = False
             comment = Comment(created_by=User.objects.get(email=email),professor=Professor.objects.get(pk=i['professor_id']),body = i['content'],created_at=datetime.datetime.today(),is_anonymous = jd,responsibility=i['rating_1'],workload = i['rating_2'],difficulty=i['rating_3'],personality=i['rating_4'])
             comment.save()